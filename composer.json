{
    "name": "thisisharshoriya5565/laravel-sitemap-generator",
    "description": "A simple sitemap generator for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "you@example.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Vendor\\LaravelSitemap\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vendor\\LaravelSitemap\\LaravelSitemapServiceProvider"
            ],
            "aliases": {
                "Sitemap": "Vendor\\LaravelSitemap\\Facades\\Sitemap"
            }
        }
    }
}